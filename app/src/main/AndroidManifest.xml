<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.usc.parknpay">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".authentication.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".authentication.RegistrationActivity" />
        <activity android:name=".seeker.SeekerMainActivity" />
        <activity android:name=".owner.OwnerMainActivity" />
        <activity android:name=".authentication.SetDefaultModeActivity"></activity>

        <activity
            android:name=".mutual.PaymentActivity"
            android:label="Payment"
            android:theme="@style/AppBarActivity">
        </activity>

    </application>

</manifest>